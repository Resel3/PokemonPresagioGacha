def gachaponRead
  prob = rand(100)
  case prob
  when (0...41)
    items = [:POKEBALL, :FULLHEAL]
    result = items[rand(items.length)]
    scene.itemReward(result,1)
  when (41...71)
    items = [:GREATBALL, :SUPERPOTION]
    result = items[rand(items.length)]
    scene.itemReward(result,2)
  when (71...91)
    items = [:ULTRABALL, :HYPERPOTION]
    result = items[rand(items.length)]
    scene.itemReward(result,3)
  when (91...100)
    items = [:MASTERBALL, :RARECANDY]
    result = items[rand(items.length)]
    scene.itemReward(result,4)
  end
end
