prob = rand(100)
case prob
#############################################
# COMUN con un 41%
#############################################
when (0...41) 
  items = [:POKEBALL, :FULLHEAL]
  result = items[rand(items.length)]
  itemReward(result,1)
#############################################
# RARO con un 15%
#############################################
when (41...56) 
  itemreward=[PBItems::GREATBALL]
  result = items[rand(items.length)]
#############################################
# RARO con un 15%
#############################################
when (56...71) 
  itemreward=[PBItems::SUPERPOTION]
  result = items[rand(items.length)]
#############################################
# Ã‰PICO con un 20%
#############################################
when (71...91) 
   result = rand(10)
#############################################
# LEGENDARIO con un 9%
#############################################
when (91...100) 
   result = rand(10)
end
